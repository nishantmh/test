   <script type="text/ng-template" id="/register.html">
  <div class="page-header">
    <h1>GPS Tracker</h1>
  </div>

  <div ng-show="error" class="alert alert-danger row">
    <span>{{ error.message }}</span>
  </div>

  <form ng-submit="register()"
    style="margin-top:30px;">
    <h3>Register</h3>

    <div class="form-group">
      <input type="text"
      class="form-control"
      placeholder="Username"
      ng-model="user.username"></input>
    </div>
    <div class="form-group">
      <input type="password"
      class="form-control"
      placeholder="Password"
      ng-model="user.password"></input>
    </div>
    <button type="submit" class="btn btn-primary">Register</button>
  </form>
</script>

<script type="text/ng-template" id="/login.html">
  <div class="page-header">
    <h1>GPS Logger</h1>
  </div>

  <div ng-show="error" class="alert alert-danger row">
    <span>{{ error.message }}</span>
  </div>

  <form ng-submit="logIn()"
    style="margin-top:30px;">
    <h3>Log In</h3>

    <div class="form-group">
      <input type="text"
      class="form-control"
      placeholder="Username"
      ng-model="user.username"></input>
    </div>
    <div class="form-group">
      <input type="password"
      class="form-control"
      placeholder="Password"
      ng-model="user.password"></input>
    </div>
    <button type="submit" class="btn btn-primary">Log In</button>
  </form>
  </script>

   </script>

  
    <script type="text/ng-template" id="/AdminHome.html">
       
        <div class="container-fluid">
        <div class="row titleBar">
            <div class="col-sm-8 col-xs-8 col-sm-offset-2 col-xs-offset-2 text-center">
                GPS Tracker
            </div>
            <div class="col-sm-2 col-xs-2" style="padding-top: 4px; text-align: right;">
                <i class="fa fa-power-off"></i>
            </div>
        </div>
        <div class="col-sm-9 col-xs-12 mid no-padding">
            <ul class="nav nav-pills tab-pills">
                <li class="active"><a data-toggle="pill" href="#addCusto">Add Customer</a></li>
                <li><a data-toggle="pill" href="#addDev">Add Device</a></li>
                <li><a data-toggle="pill" href="#mapDev">Map Device</a></li>
            </ul>

            <div class="tab-content">
                <div id="addCusto" class="tab-pane fade in active">
                    <div class="col-sm-12 col-xs-12 tabContainer no-padding">
                        <h2 class="fRColor text-center">Add Customer</h2>
                        <hr />
                        <div class="col-sm-8 col-xs-12 mid no-padding">
                            <div class="col-sm-12 col-xs-12 form-group">
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <label for="cEmail" class="lblText">Customer Email:</label>
                                </div>
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <input type="email" class="form-control" id="email" placeholder="Email">
                                </div>
                            </div>
                            <div class="col-sm-12 col-xs-12 form-group">
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <label for="name" class="lblText">Password:</label>
                                </div>
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <input type="pass" class="form-control" id="pass" placeholder="password">
                                </div>
                            </div>
                            <div class="col-sm-12 col-xs-12 text-center form-group">
                                <a href="index.html" class="btn bgRColor fColor btnWidth">Add</a>
                                <a href="index.html" class="btn bgRColor fColor btnWidth">Reset</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="addDev" class="tab-pane fade">
                    <div class="col-sm-12 col-xs-12 tabContainer no-padding">
                        <h2 class="fRColor text-center">Add Device</h2>
                        <hr />
                        <div class="col-sm-8 col-xs-12 mid no-padding">
                            <div class="col-sm-12 col-xs-12 form-group">
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <label for="cEmail" class="lblText">Device ID:</label>
                                </div>
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <input type="number" class="form-control" id="dId" placeholder="Device ID">
                                </div>
                            </div>
                            <div class="col-sm-12 col-xs-12 form-group">
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <label for="name" class="lblText">Device Name:</label>
                                </div>
                                <div class="col-sm-6 col-xs-6 no-padding">
                                    <input type="text" class="form-control" id="dName" placeholder="Device Name">
                                </div>
                            </div>
                            <div class="col-sm-12 col-xs-12 text-center form-group">
                                <a href="index.html" class="btn bgRColor fColor btnWidth">Add</a>
                                <a href="index.html" class="btn bgRColor fColor btnWidth">Reset</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="mapDev" class="tab-pane fade">
                    <div class="col-sm-12 col-xs-12 tabContainer no-padding">
                        <h2 class="fRColor text-center">Map Device To Customer</h2>
                        <hr />
                        <div class="col-sm-8 col-xs-12 mid no-padding">
                            <div class="col-sm-12 col-xs-12 form-group">
                                <div class="col-sm-6 col-xs-6">
                                    <label for="cEmail" class="lblText">Customer:</label>
                                </div>
                                <div class="col-sm-6 col-xs-6">
                                    <select class="form-control" id="sel1">
                                        <option>option 1</option>
                                        <option>option 2</option>
                                        <option>option 3</option>
                                        <option>option 4</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-12 col-xs-12 form-group">
                                <div class="col-sm-6 col-xs-6">
                                    <label for="name" class="lblText">Device:</label>
                                </div>
                                <div class="col-sm-6 col-xs-6">
                                    <select class="form-control" id="Select1">
                                        <option>option 1</option>
                                        <option>option 2</option>
                                        <option>option 3</option>
                                        <option>option 4</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-12 col-xs-12 text-center form-group">
                                <a href="index.html" class="btn bgRColor fColor btnWidth">Add</a>
                                <a href="index.html" class="btn bgRColor fColor btnWidth">Reset</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </script>
	
</body>
